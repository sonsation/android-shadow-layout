<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShadowLayout">
        <attr name="autoAdjustPadding" format="boolean"/>
        <attr name="clipToOutline" format="boolean"/>
        <attr name="background_blur" format="dimension" />
        <attr name="background_radius_weight" format="float" />
        <attr name="background_radius" format="dimension" />
        <attr name="background_radius_half" format="boolean" />
        <attr name="background_top_left_radius" format="dimension" />
        <attr name="background_top_right_radius" format="dimension" />
        <attr name="background_bottom_left_radius" format="dimension" />
        <attr name="background_bottom_right_radius" format="dimension" />
        <attr name="background_color" format="color" />
        <attr name="android:background" format="integer"/>
        <attr name="android:alpha" format="float"/>
        <attr name="background_blur_type" format="enum">
            <enum name="NORMAL" value="0"/>
            <enum name="SOLID" value="1"/>
            <enum name="OUTER" value="2"/>
            <enum name="INNER" value="3"/>
        </attr>

        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_gradient_start_color" format="color"/>
        <attr name="stroke_gradient_center_color" format="color"/>
        <attr name="stroke_gradient_end_color" format="color"/>
        <attr name="stroke_gradient_angle" format="integer"/>
        <attr name="stroke_gradient_offset_x" format="dimension"/>
        <attr name="stroke_gradient_offset_y" format="dimension"/>
        <attr name="stroke_gradient_colors" format="string"/>
        <attr name="stroke_gradient_positions" format="string"/>
        <attr name="stroke_blur" format="dimension"/>
        <attr name="stroke_blur_type" format="enum">
            <enum name="NORMAL" value="0"/>
            <enum name="SOLID" value="1"/>
            <enum name="OUTER" value="2"/>
            <enum name="INNER" value="3"/>
        </attr>
        <attr name="stroke_type" format="enum">
            <enum name="INSIDE" value="0"/>
            <enum name="CENTER" value="1"/>
            <enum name="OUTSIDE" value="2"/>
        </attr>

        <attr name="shadow_color" format="color" />
        <attr name="shadow_offset_x" format="dimension" />
        <attr name="shadow_offset_y" format="dimension" />
        <attr name="shadow_blur" format="dimension"/>
        <attr name="shadow_spread" format="dimension" />
        <attr name="shadow_array" format="string"/>

        <attr name="gradient_start_color" format="color"/>
        <attr name="gradient_center_color" format="color"/>
        <attr name="gradient_end_color" format="color"/>
        <attr name="gradient_angle" format="integer"/>
        <attr name="gradient_offset_x" format="dimension"/>
        <attr name="gradient_offset_y" format="dimension"/>
        <attr name="gradient_margin" format="dimension"/>
        <attr name="gradient_colors" format="string"/>
        <attr name="gradient_positions" format="string"/>
    </declare-styleable>
</resources>